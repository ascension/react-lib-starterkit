{
  "name": "react-lib-starterkit",
  "version": "1.0.0",
  "description": "Starterkit for React libraries",
  "main": "dist/react-lib-starterkit.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup-babel-lib-bundler src/index.js",
    "lint": "eslint .",
    "pretest": "npm run build && npm run lint",
    "test": "karma start",
    "precommit": "npm run lint",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostney/react-lib-starterkit.git"
  },
  "keywords": [
    "react",
    "lib",
    "starterkit",
    "boilerplate"
  ],
  "author": "Johannes Stein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostney/react-lib-starterkit/issues"
  },
  "homepage": "https://github.com/frostney/react-lib-starterkit#readme",
  "dependencies": {
    "react": "^0.14.5"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-preset-es2015-loose-rollup": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "buffer": "^4.5.0",
    "chai": "^3.4.1",
    "enzyme": "^2.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.0.0",
    "husky": "^0.11.1",
    "karma": "^0.13.21",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-rollup-preprocessor": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^2.3.4",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.5",
    "react-dom": "^0.14.5",
    "rimraf": "^2.5.2",
    "rollup-babel-lib-bundler": "^2.1.1",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0"
  }
